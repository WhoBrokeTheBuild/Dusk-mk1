			<Add library="X11" />
			<Add library="Xi" />
			<Add library="Xxf86vm" />
			<Add library="Xrandr" />
			<Add library="pthread" />
		</Linker>
		<Unit filename="Main.cpp">
			<Option virtualFolder="Project/" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
